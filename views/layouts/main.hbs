{{> header}}


<h1 class="header">{{Titulo}}</h1>

<div class="container">
   
    <div class="Tabla">
        <table class="Resultado">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Nombre</th>
                    <th>Precio</th>
                    <th>Stock</th>

                </tr>
            </thead>
            <tbody class="tableBody" id="tableBody" >

            </tbody>
        </table>
    </div>

    <div class="Chat Resultado">
        <h2>Chat</h2>
        <div class="chat">
            <div class="chat-container">
                <div class="chat-container-header">
                    <div class="chat-container-header-title">
                        <h3>Chat</h3>
                    </div>
                    <div class="chat-container-header-close">
                        <button class="btn btn-danger">Cerrar</button>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    
    <script src="/index.js"></script>
</div>



{{> footer}}